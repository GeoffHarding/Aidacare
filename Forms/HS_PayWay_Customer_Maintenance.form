	'HS PayWay Customer CB' = true;
	'Force PayWay Details From Address' = 'Force PayWay Details From Address' of table HS_PayWay_Customer_Address_MSTR;
	'HS PayWay Customer Reference Number' = 'HS PayWay Customer Reference Number' of table HS_PayWay_Customer_Address_MSTR;
	'Allow Customer Reference Changes' = 'Allow Customer Reference Changes' of table HS_PayWay_Customer_Address_MSTR;
else
	'HS PayWay Customer CB' = false;
	clear 'Force PayWay Details From Address';
	clear 'HS PayWay Customer Reference Number';
	clear 'Allow Customer Reference Changes';
end if;


run script 'HS PayWay Customer CB';
