		EventScript	"PayWay_Customer_Maintenance Save Button_CHG"		
		{
			Documentation	""
			Script	"call Delete_PayWay_Customer_Address, CH_1, 'User ID' of globals;


if 'HS PayWay Customer CB' then
	if not 'Force PayWay Details From Address' and empty('HS PayWay Customer Reference Number') then
		warning getmsg(22008);   {Please enter a Customer Reference Number.}
		abort script;
	end if;
	
	'Force PayWay Details From Address' of table HS_PayWay_Customer_Address_MSTR = 'Force PayWay Details From Address';
	'HS PayWay Customer Reference Number' of table HS_PayWay_Customer_Address_MSTR = 'HS PayWay Customer Reference Number';
	'Allow Customer Reference Changes' of table HS_PayWay_Customer_Address_MSTR = 'Allow Customer Reference Changes';
	save table HS_PayWay_Customer_Address_MSTR;
	if err() = DUPLICATE then
		warning getmsg(22009);   {Please enter a unique Customer Reference Number.}
		
		'Customer Number' of table HS_PayWay_Customer_Address_MSTR = 'Customer Number';
		clear 'Customer Number' of table HS_PayWay_Customer_Address_MSTR;
		change table HS_PayWay_Customer_Address_MSTR;
		abort script;
	end if;
else
	release table HS_PayWay_Customer_Address_MSTR;
	
	'Customer Number' of table HS_PayWay_Customer_Address_MSTR = 'Customer Number';
	clear 'Customer Number' of table HS_PayWay_Customer_Address_MSTR;
	change table HS_PayWay_Customer_Address_MSTR;
	if err() = OKAY then
		remove table HS_PayWay_Customer_Address_MSTR;
	else
		release table HS_PayWay_Customer_Address_MSTR;
	end if;
end if;

close form HS_PayWay_Customer_Maintenance;
