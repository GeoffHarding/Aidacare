		{
			Documentation	""
			Script	"local	integer		nYear;
local	integer		nLoop;


nYear = year('User Date' of globals);
nYear = nYear - 1;


clear '(L) Expiry Year DDL';
for nLoop = nYear to (nYear + 9) do
	add item pad(str(nLoop % 100), LEADING, CH_0, 2)  to '(L) Expiry Year DDL';
end for;


call Delete_PayWay_Customer_Address, CH_1, 'User ID' of globals;
