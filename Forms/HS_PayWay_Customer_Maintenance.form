		EventScript	"PayWay_Customer_Maintenance l_Summary Code_CHG"		
		{
			Documentation	""
			Script	"pragma (disable warning LiteralStringUsed);

case '(L) Response Code'
	in [\"00\"]
		warning getmsg(22013);   {Customer and credit card details have been successfully updated in PayWay.}
		run script 'Save Button';
		abort script;
		
	in [\"14\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Invalid card number (no such number)}
		
	in [\"QY\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Card Type Not Accepted}
		
	in [\"54\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Expired card}
		
	else
		warning getmsg(22014);   {Error updating PayWay.}
		
end case;

pragma (enable warning LiteralStringUsed);
"
			UserProperties	""
		}
		EventScript	"PayWay_Customer_Maintenance l_Update Credit Card_CHG"		
		{
			Documentation	""
			Script	"clear '(L) PayWay Register Customer';

if empty('HS PayWay Customer Reference Number') then
	warning getmsg(22006);   {Please enter the Customer Reference Number first.}
	abort script;
end if;

if empty('(L) Card Holder Name') or empty('(L) Card Number') or empty('(L) Expiry Month') or empty('(L) Expiry Year') then
	warning getmsg(22007);   {Please enter all credit card details first.}
	abort script;
end if;


'(L) PayWay Register Customer' = 1;
"
