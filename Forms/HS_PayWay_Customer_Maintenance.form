		{
			Documentation	""
			Script	"call Delete_PayWay_Customer_Address, CH_1, 'User ID' of globals;


if not Is_PayWay_Reference_Unique('HS PayWay Customer Reference Number', 'Customer Number', \"\") then
	warning getmsg(22009);   {Please enter a unique Customer Reference Number.}
	'HS PayWay Customer Reference Number' = old();
