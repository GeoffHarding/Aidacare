	~EventScripts
	{
		EventScript	"PayWay_Address_Maintenance Address Code_CHG"		
		{
			Documentation	""
			Script	"'Customer Number' of table RM_Customer_MSTR = 'Customer Number';
get table RM_Customer_MSTR;
if err() <> OKAY then
	close form HS_PayWay_Address_Maintenance;
end if;

'Customer Name' = 'Customer Name' of table RM_Customer_MSTR;


'Customer Number' of table HS_PayWay_Customer_Address_MSTR = 'Customer Number';
'Address Code' of table HS_PayWay_Customer_Address_MSTR = 'Address Code';
change table HS_PayWay_Customer_Address_MSTR;
if err() = OKAY then
	'HS PayWay Customer CB' = true;
	'HS PayWay Customer Reference Number' = 'HS PayWay Customer Reference Number' of table HS_PayWay_Customer_Address_MSTR;
	'PayWay Login ID' = 'PayWay Login ID' of table HS_PayWay_Customer_Address_MSTR;
	if empty('PayWay Login ID') then
		'(L) Login From' = 0;
	else
		'(L) Login From' = 1;
	end if;
else
	'HS PayWay Customer CB' = false;
	clear 'HS PayWay Customer Reference Number';
	'(L) Login From' = 0;
end if;


run script 'HS PayWay Customer CB';
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance Cancel Button_CHG"		
		{
			Documentation	""
			Script	"close form HS_PayWay_Address_Maintenance;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance HS PayWay Customer CB_CHG"		
		{
			Documentation	""
			Script	"if 'HS PayWay Customer CB' then
	unlock 'HS PayWay Customer Reference Number';
	unlock '(L) Login From';
	unlock '(L) Card Holder Name';
	unlock '(L) Card Number';
	unlock '(L) Expiry Month DDL';
	unlock '(L) Expiry Year DDL';
	unlock '(L) Update Credit Card';
	enable '(L) Generate Customer Reference';
	
	run script '(L) Login From';
	run script '(L) Lock Customer Reference';
else
	lock 'HS PayWay Customer Reference Number';
	lock '(L) Login From';
	lock '(L) Card Holder Name';
	lock '(L) Card Number';
	lock '(L) Expiry Month DDL';
	lock '(L) Expiry Year DDL';
	lock '(L) Update Credit Card';
	disable '(L) Generate Customer Reference';
	
	clear 'HS PayWay Customer Reference Number';
	'(L) Login From' = 0;
	run script '(L) Login From';
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance HS PayWay Customer Reference Number_CHG"		
		{
			Documentation	""
			Script	"call Delete_PayWay_Customer_Address, CH_2, 'User ID' of globals;


if not Is_PayWay_Reference_Unique('HS PayWay Customer Reference Number', 'Customer Number', 'Address Code') then
	warning getmsg(22009);   {Please enter a unique Customer Reference Number.}
	'HS PayWay Customer Reference Number' = old();
	restart field;
	abort script;
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance Lookup Button 1_CHG"		
		{
			Documentation	""
			Script	"open form HS_PayWay_Login_Lookup return to 'PayWay Login ID';
call INITIALIZE of form HS_PayWay_Login_Lookup, 1, 'PayWay Login ID', \"\";
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance PayWay Login ID_CHG"		
		{
			Documentation	""
			Script	"local	string		sMsg;


if empty('PayWay Login ID') then	
	abort script;
end if;

'PayWay Login ID' of table HS_PayWay_Logins_SETP = 'PayWay Login ID';
get table HS_PayWay_Logins_SETP;
if err() <> OKAY then
	restart field;
	
	sMsg = getmsg(22047);   {Do you want to add this %1?}
	substitute sMsg, getmsg(22041);   {Login ID}
	if ask(sMsg, getmsg(YES), getmsg(NO), \"\") = ASKBUTTON1 then
		open form HS_PayWay_Setup;
		call INITIALIZE of form HS_PayWay_Login_Setup, 'PayWay Login ID';
	end if;
	abort script;
end if; 
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance Save Button_CHG"		
		{
			Documentation	""
			Script	"call Delete_PayWay_Customer_Address, CH_2, 'User ID' of globals;


if 'HS PayWay Customer CB' then
	if empty('HS PayWay Customer Reference Number') then
		warning getmsg(22008);   {Please enter a Customer Reference Number.}
		abort script;
	end if;
	
	'HS PayWay Customer Reference Number' of table HS_PayWay_Customer_Address_MSTR = 'HS PayWay Customer Reference Number';
	'PayWay Login ID' of table HS_PayWay_Customer_Address_MSTR = 'PayWay Login ID';
	save table HS_PayWay_Customer_Address_MSTR;
	if err() = DUPLICATE then
		warning getmsg(22009);   {Please enter a unique Customer Reference Number.}
		
		'Customer Number' of table HS_PayWay_Customer_Address_MSTR = 'Customer Number';
		'Address Code' of table HS_PayWay_Customer_Address_MSTR = 'Address Code';
		change table HS_PayWay_Customer_Address_MSTR;
		abort script;
	end if;
else
	release table HS_PayWay_Customer_Address_MSTR;
	
	'Customer Number' of table HS_PayWay_Customer_Address_MSTR = 'Customer Number';
	'Address Code' of table HS_PayWay_Customer_Address_MSTR = 'Address Code';
	change table HS_PayWay_Customer_Address_MSTR;
	if err() = OKAY then
		remove table HS_PayWay_Customer_Address_MSTR;
	else
		release table HS_PayWay_Customer_Address_MSTR;
	end if;
end if;

close form HS_PayWay_Address_Maintenance;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Expiry Month DDL_CHG"		
		{
			Documentation	""
			Script	"'(L) Expiry Month' = itemname('(L) Expiry Month DDL', '(L) Expiry Month DDL');

"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Expiry Year DDL_CHG"		
		{
			Documentation	""
			Script	"'(L) Expiry Year' = itemname('(L) Expiry Year DDL', '(L) Expiry Year DDL');
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Generate Customer Reference_CHG"		
		{
			Documentation	""
			Script	"local	long		lStatus;
local	string		sCustomerReferenceNumber;


if not empty('HS PayWay Customer Reference Number') then
	warning getmsg(22037);   {A Customer Reference Number is already being used.}
	abort script;
end if;


call hsPayWayGenerateCustomerReference,
 lStatus,
 CH_2,
 'User ID' of globals,
 sCustomerReferenceNumber;


if not empty(sCustomerReferenceNumber) then
	'HS PayWay Customer Reference Number' = sCustomerReferenceNumber;
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Lock Customer Reference_CHG"		
		{
			Documentation	""
			Script	"{ This is a code stub so that Aidacare can trigger off of to lock/unlock the
 Customer Reference and Auto-Generate button depending on User/Class }


pragma (disable warning NoExecutableCode);
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Login From_CHG"		
		{
			Documentation	""
			Script	"if '(L) Login From' = 0 then
	clear 'PayWay Login ID';
	hide 'PayWay Login ID';
	hide 'Lookup Button 1';
else
	show 'PayWay Login ID';
	show 'Lookup Button 1';
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Summary Code_CHG"		
		{
			Documentation	""
			Script	"pragma (disable warning LiteralStringUsed);

case '(L) Response Code'
	in [\"00\"]
		warning getmsg(22013);   {Customer and credit card details have been successfully updated in PayWay.}
		run script 'Save Button';
		abort script;
		
	in [\"14\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Invalid card number (no such number)}
		
	in [\"QY\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Card Type Not Accepted}
		
	in [\"54\"]
		warning getmsg(22017) + '(L) Response Text';   {PayWay update failed - Expired card}
		
	else
		warning getmsg(22014);   {Error updating PayWay.}
		
end case;

pragma (enable warning LiteralStringUsed);
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance l_Update Credit Card_CHG"		
		{
			Documentation	""
			Script	"clear '(L) PayWay Register Customer';

if empty('HS PayWay Customer Reference Number') then
	warning getmsg(22006);   {Please enter the Customer Reference Number first.}
	abort script;
end if;

if empty('(L) Card Holder Name') or empty('(L) Card Number') or empty('(L) Expiry Month') or empty('(L) Expiry Year') then
	warning getmsg(22007);   {Please enter all credit card details first.}
	abort script;
end if;


'(L) PayWay Register Customer' = 1;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance_WIN_POST"		
		{
			Documentation	""
			Script	"call Delete_PayWay_Customer_Address, CH_2, 'User ID' of globals;
"
			UserProperties	""
		}
		EventScript	"PayWay_Address_Maintenance_WIN_PRE"		
		{
			Documentation	""
			Script	"local	integer		nYear;
local	integer		nLoop;


nYear = year('User Date' of globals);
nYear = nYear - 1;


clear '(L) Expiry Year DDL';
for nLoop = nYear to (nYear + 9) do
	add item pad(str(nLoop % 100), LEADING, CH_0, 2)  to '(L) Expiry Year DDL';
end for;


call Delete_PayWay_Customer_Address, CH_2, 'User ID' of globals;
"
			UserProperties	""
		}
	}
