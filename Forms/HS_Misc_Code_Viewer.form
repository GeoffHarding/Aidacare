		Procedure	"OPEN_CODE_VIEWER"		
		{
			Documentation	""
			Script	"in		text		IN_Code;
in		string		IN_Compiler_Errors;


if isopen(form HS_Misc_Code_Viewer) then
	open form HS_Misc_Code_Viewer;
	abort script;
end if;


default window to Code_Viewer;

case upper(Defaults_Read(HS_VIEW_EXECUTE_CODE))
	in [TRUE_STRING]
	in [ERRORS]
		if empty(IN_Compiler_Errors) or IN_Compiler_Errors = CH_0 then
			abort script;
		end if;
	else
		abort script;
end case;


open form HS_Misc_Code_Viewer;
if isopen(form HS_Misc_Code_Viewer) then
	'(L) Code'[1] = IN_Code;
	'(L) Code Text' = '(L) Code'[1];
	'(L) Compiler Errors' = IN_Compiler_Errors;
	'(L) Max Array' = 1;
	hide 'Previous Button - Window Area';
	hide 'Next Button - Window Area';
	hide '(L) Array String';
end if;
"
			UserProperties	""
		}
		Procedure	"OPEN_SQL_VIEWER"		
		{
			Documentation	""
			Script	"in				text			IN_SQL_Code[10];
in				long			IN_Compiler_Errors;
optional in		long			IN_SQL_Connection;

local			integer			lLoop;
local			long			lSQLConnection;
local			long			lStatus;
local			long 			lGPSErrorNumber;
local			long 			lSQLErrorNumber;
local			string			sSQLErrorString;
local			string			sODBCErrorString;
local			string			sCompilerErrors;


if isopen(form HS_Misc_Code_Viewer) then
	open form HS_Misc_Code_Viewer;
	abort script;
end if;


default window to Code_Viewer;

case upper(Defaults_Read(HS_VIEW_EXECUTE_CODE))
	in [TRUE_STRING]
	in [ERRORS]
		if empty(IN_Compiler_Errors) then
			abort script;
		end if;
	else
		abort script;
end case;


open form HS_Misc_Code_Viewer;
if isopen(form HS_Misc_Code_Viewer) then
	for lLoop = 1 to 10 do
		'(L) Code'[lLoop] = IN_SQL_Code[lLoop];
		if not empty(IN_SQL_Code[lLoop]) then
			'(L) Max Array' = lLoop;
		end if;
	end for;
	'(L) Code Text' = '(L) Code'[1];
	
	if not missing(IN_SQL_Connection) then
		lSQLConnection = IN_SQL_Connection;
	else
		lSQLConnection = 'SQLFrontHandle' of globals;
	end if;
	
	lStatus = SQL_GetError(lSQLConnection, lGPSErrorNumber, lSQLErrorNumber, sSQLErrorString, sODBCErrorString);
	sCompilerErrors = getmsg(22100);   {SQL Server returned the following error number:  %1.  %2.}
	substitute sCompilerErrors, str(lSQLErrorNumber), sSQLErrorString;
	'(L) Compiler Errors' = sCompilerErrors;
	
	if '(L) Max Array' > 1 then
		'(L) Array' = 1;
		'(L) Array String' = str('(L) Array');
	else
		hide 'Previous Button - Window Area';
		hide 'Next Button - Window Area';
		hide '(L) Array String';
	end if;
end if;

pragma (enable warning LiteralStringUsed);
"
			UserProperties	""
		}
	}
