	~Functions
	{
		Function	"Is_Unique_User_Name"		
		{
			Documentation	""
			Script	"function returns boolean OUT_Unique;
in		string		IN_PayWay_User_Name;
in		string		IN_PayWay_Login_ID;


OUT_Unique = true;

'PayWay User Name' of table HS_PayWay_Logins_SETP = IN_PayWay_User_Name;
get table HS_PayWay_Logins_SETP by number 2;
if err() = OKAY then
	if 'PayWay Login ID' of table HS_PayWay_Logins_SETP <> IN_PayWay_Login_ID then
		OUT_Unique = false;
	end if;
end if;
"
			UserProperties	""
		}
	}
	~Procedures
	{
		Procedure	"Display_Existing_Record"		
		{
			Documentation	""
			Script	"inout	table		HS_PayWay_Logins_SETP;
out		boolean		OUT_Existing;


{--------------------------------------------------------------------------------
	This script reads and displays the newly selected record.

	This script is called from two different locations:
		(1)	The '(L) Temp Control Number' change script.
		(2)	The control number change script.

	This script sets itself as a flag indicating whether or not
	an existing record is being edited.
--------------------------------------------------------------------------------}

{--------------------------------------------------------------------------------
	Read the newly selected record.
--------------------------------------------------------------------------------}
'PayWay Login ID' of table HS_PayWay_Logins_SETP = 'PayWay Login ID' of window Login_Setup;
change table HS_PayWay_Logins_SETP;

{--------------------------------------------------------------------------------
	Display the information for the record if it already exists.
--------------------------------------------------------------------------------}
if err() <> MISSING then
	copy from table HS_PayWay_Logins_SETP;
	clear changes window Login_Setup;
	OUT_Existing = true;
else
	OUT_Existing = false;
end if;


{--------------------------------------------------------------------------------
	Lock the control number field.
--------------------------------------------------------------------------------}
lock 'PayWay Login ID' of window Login_Setup;
"
			UserProperties	""
		}
		Procedure	"Handle_Changes"		
		{
			Documentation	""
			Script	"inout	table		HS_PayWay_Logins_SETP;
out		boolean		OUT_Handled;

local	boolean		bSaved;
local	integer		nAsk;


{--------------------------------------------------------------------------------
	This script will handle changes to the record presently being displayed.

	This script is called from two different locations:
		(1)	The '(L) Temp Control Number' change script.
		(2)	The window post script.

	This script sets itself as a flag which indicates whether or not
	the calling script should continue after running this script.
--------------------------------------------------------------------------------}

{--------------------------------------------------------------------------------
	Ask the if the user wishes to save the changes or cancel whatever action is
	being taken.
--------------------------------------------------------------------------------}
nAsk = ask(getmsg(1000){Do you want to save changes?}, getmsg(2){&Save}, getmsg(4) {&Discard}, getmsg(1){&Cancel});

if nAsk = ASKBUTTON1 then
	{----------------------------------------------------------------------------
		The user has chosen to save the changes to the record.
	----------------------------------------------------------------------------}
	{----------------------------------------------------------------------------
		Try to save the record.  The record will not be saved if a required field
		is empty or if there is a collision.
	----------------------------------------------------------------------------}
	call Save_Record of form HS_PayWay_Login_Setup, table HS_PayWay_Logins_SETP, bSaved;
	if bSaved then
		OUT_Handled = true;
	else
		focus 'Save Button' of window Login_Setup;
		OUT_Handled = false;
	end if;

elseif nAsk = ASKBUTTON2 then
	{----------------------------------------------------------------------------
		The user has chosen not to save the record.
	----------------------------------------------------------------------------}
	release table HS_PayWay_Logins_SETP;
	OUT_Handled = true;

else
	{----------------------------------------------------------------------------
		The user has chosen to cancel the action being taken.
	----------------------------------------------------------------------------}
	focus 'Save Button' of window Login_Setup;
	OUT_Handled = false;
end if;
"
			UserProperties	""
		}
		Procedure	"INITIALIZE"		
		{
			Documentation	""
			Script	"in		'PayWay Login ID'			IN_Login_ID;


open window Login_Setup;

set '(L) Temp Control Number' of window Login_Setup to IN_Login_ID;
run script delayed '(L) Temp Control Number' of window Login_Setup;
"
			UserProperties	""
		}
		Procedure	"Save_Record"		
		{
			Documentation	""
			Script	"inout	table		HS_PayWay_Logins_SETP;
out		boolean		OUT_Saved;

{--------------------------------------------------------------------------------
	This script will save the record if all required fields have been entered.

	This script is called from two different locations:
		(1)	The 'Save Button' change script.
		(2)	The 'Handle Changes' change script.

	This script sets itself as a flag which indicates whether or not
	the record was successfully saved.
--------------------------------------------------------------------------------}

{--------------------------------------------------------------------------------
	Verify that all required fields have been entered.
--------------------------------------------------------------------------------}
if not required(window Login_Setup) then
	call Warning_Dialog,2351,\"\",\"\",\"\";
	focus 'Save Button' of window Login_Setup;
	OUT_Saved = false;
	abort script;
end if;

{--------------------------------------------------------------------------------
	Try to save the record to the table.
--------------------------------------------------------------------------------}
if day('Created Date' of table HS_PayWay_Logins_SETP) = 0 then
	'Created Date' of table HS_PayWay_Logins_SETP = sysdate();
end if;
'Modified User ID' of table HS_PayWay_Logins_SETP = 'User ID' of globals;
'Modified Date' of table HS_PayWay_Logins_SETP = sysdate();

copy from window Login_Setup to table HS_PayWay_Logins_SETP;
save table HS_PayWay_Logins_SETP;

{--------------------------------------------------------------------------------
	If the record was not saved due to a collision, then re-read the record.
--------------------------------------------------------------------------------}
if err() = RECORDCHANGED then
	call Warning_Dialog,5202,\"\",\"\",\"\";
	clear changes window Login_Setup;
	run script 'PayWay Login ID' of window Login_Setup;
	focus 'Save Button' of window Login_Setup;
	OUT_Saved = false;
	abort script;
end if;

{--------------------------------------------------------------------------------
	Check if another user has created a record with the same control number.
--------------------------------------------------------------------------------}
if err() = DUPLICATE then
	call Warning_Dialog,5283,\"\",\"\",\"\";
	unlock 'PayWay Login ID' of window Login_Setup;
	focus 'PayWay Login ID' of window Login_Setup;
	OUT_Saved = false;
	abort script;
end if;

{--------------------------------------------------------------------------------
	 The record has been successfully saved.
--------------------------------------------------------------------------------}
OUT_Saved = true;
"
			UserProperties	""
		}
	}
}
