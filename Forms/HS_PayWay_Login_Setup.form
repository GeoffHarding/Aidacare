--------------------------------------------------------------------------------}
if changed(window Login_Setup) then
	call Handle_Changes of form HS_PayWay_Login_Setup, table HS_PayWay_Logins_SETP, bHandled;
	if not bHandled then
		abort close;
	end if;
end if;
"
			UserProperties	""
		}
