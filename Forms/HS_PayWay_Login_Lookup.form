HS_LU_View_Button of window Lookup = 1;
check field HS_LU_View_Button of window Lookup, 1;
HS_LU_Current_View of window Lookup = itemname(HS_LU_View_Button of window Lookup, HS_LU_View_Button of window Lookup);


