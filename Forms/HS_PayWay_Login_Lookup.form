	~Procedures
	{
		Procedure	"INITIALIZE"		
		{
			Documentation	""
			Script	"in		'Sort By'				IN_Sort_By;
in		'PayWay Login ID'		IN_Login_ID;
in		'PayWay User Name'		IN_User_Name;


'Sort By' of window Lookup = IN_Sort_By;

'PayWay Login ID' of window Lookup = IN_Login_ID;
'PayWay User Name' of window Lookup = IN_User_Name;

case 'Sort By' of window Lookup
	in [1]
		'PayWay Login ID' of window Lookup = IN_Login_ID;
		run script 'PayWay Login ID' of window Lookup;
		HS_LU_Current_Sort_String of window Lookup = getmsg(22043); { by Login ID }
	in [2]
		'PayWay User Name' of window Lookup = IN_User_Name;
		run script 'PayWay User Name' of window Lookup;
		HS_LU_Current_Sort_String of window Lookup = getmsg(22044); { by User Name }
	else
		'Sort By' of window Lookup = 1;
		run script 'Sort By' of window Lookup;
		HS_LU_Current_Sort_String of window Lookup = getmsg(22043); { by Login ID }
end case;

run script HS_LU_Set_View_Prompt of window Lookup;
"
			UserProperties	""
		}
