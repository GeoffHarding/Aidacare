		clear table HS_PayWay_Logins_SETP;
		if not empty('PayWay Login ID') then
			'PayWay Login ID' of table HS_PayWay_Logins_SETP = 'PayWay Login ID';
			fill window Lookup_Scroll by number 'Sort By';
			clear field 'PayWay Login ID';
		else
			fill window Lookup_Scroll by number 'Sort By';
		end if;
	else  {'Sort By' = 2}
		if not empty('PayWay User Name') then
			clear table HS_PayWay_Logins_SETP;
			'PayWay User Name' of table HS_PayWay_Logins_SETP = 'PayWay User Name';
			fill window Lookup_Scroll by number 'Sort By';
			clear 'PayWay User Name';
		else
			fill window Lookup_Scroll by number 'Sort By';
		end if;
end case;

focus 'PayWay Login ID' of window Lookup_Scroll;
"
			UserProperties	""
		}
