		EventScript	"Lookup HS_LU_Set_View_Prompt_CHG"		
		{
			Documentation	""
			Script	"local	string		sPrompt;
local	boolean		bCaptionset;
{local	integer		nDatatype;
local	integer		nCount, i;}


sPrompt = getmsg(22045); { &View: %1; %2 }
substitute sPrompt, HS_LU_Current_View, HS_LU_Current_Sort_String;
bCaptionset = Field_SetCaption(HS_LU_View_Button, sPrompt);
HS_LU_Find_Prompt = getmsg(22046); { Find %1 }
substitute HS_LU_Find_Prompt, HS_LU_Current_Sort_String;
redraw HS_LU_Find_Prompt;

{case 'Sort By'
	in [1]
		nDatatype = HS_DT_STRING;
	in [2]
		nDatatype = HS_DT_STRING;
end case;

case nDatatype
	in [HS_DT_DATE]
		clear HS_LU_Find_DDL;
		disable HS_LU_Find_DDL;
		hide HS_LU_Find_DDL;
		clear HS_LU_Find_Account_Number;
		disable HS_LU_Find_Account_Number;
		hide HS_LU_Find_Account_Number;
		clear HS_LU_Find_String;
		disable HS_LU_Find_String;
		hide HS_LU_Find_String;

		clear HS_LU_Find_Date;
		enable HS_LU_Find_Date;
		show HS_LU_Find_Date;
	in [HS_DT_ACCOUNTINDEX, HS_DT_ACCOUNTNUMBER]
		clear HS_LU_Find_DDL;
		disable HS_LU_Find_DDL;
		hide HS_LU_Find_DDL;
		clear HS_LU_Find_String;
		disable HS_LU_Find_String;
		hide HS_LU_Find_String;
		clear HS_LU_Find_Date;
		disable HS_LU_Find_Date;
		hide HS_LU_Find_Date;

		clear HS_LU_Find_Account_Number;
		enable HS_LU_Find_Account_Number;
		show HS_LU_Find_Account_Number;
	in [HS_DT_BOOLEAN, HS_DT_DROPDOWNLIST]
		clear HS_LU_Find_String;
		disable HS_LU_Find_String;
		hide HS_LU_Find_String;
		clear HS_LU_Find_Date;
		disable HS_LU_Find_Date;
		hide HS_LU_Find_Date;
		clear HS_LU_Find_Account_Number;
		disable HS_LU_Find_Account_Number;
		hide HS_LU_Find_Account_Number;
		
		enable HS_LU_Find_DDL;
		show HS_LU_Find_DDL;
		
		if nDatatype = HS_DT_DROPDOWNLIST then
			clear field HS_LU_Find_DDL;
			nCount = countitems(field 'EQS Equipment Category Type');
			for i = 1 to nCount do
				add item itemname('EQS Equipment Category Type', i), i to field HS_LU_Find_DDL;
			end for;
			
		end if;
	else
		clear HS_LU_Find_Date;
		disable HS_LU_Find_Date;
		hide HS_LU_Find_Date;
		clear HS_LU_Find_Account_Number;
		disable HS_LU_Find_Account_Number;
		hide HS_LU_Find_Account_Number;
		clear HS_LU_Find_DDL;
		disable HS_LU_Find_DDL;
		hide HS_LU_Find_DDL;}

		clear HS_LU_Find_String;
		enable HS_LU_Find_String;
		show HS_LU_Find_String;
{end case;}
"
			UserProperties	""
		}
