	~EventScripts
	{
		EventScript	"SOP_Payment_Entry l_PayWay Payment Button_CHG"		
		{
			Documentation	""
			Script	"clear '(L) PayWay Make Payment';


if empty('Amount Paid') then
	warning getmsg(22015);   {Please enter an amount for the payment.}
	abort script;
end if;


{ Set Card Name }
Trigger_DisableSingle('HS Trigger Tag'[TAG_SOP_PAYMENT_ENTRY_CARD_NAME_CHANGE_POST_PAYWAY] of globals);

'Dummy Record' of table HS_PayWay_SETP = false;
get table HS_PayWay_SETP;
if err() <> OKAY then
	clear table HS_PayWay_SETP;
end if;

'Card Name' = 'Card Name' of table HS_PayWay_SETP;
run script 'Card Name';

Trigger_EnableSingle('HS Trigger Tag'[TAG_SOP_PAYMENT_ENTRY_CARD_NAME_CHANGE_POST_PAYWAY] of globals);


{ Check Required Fields }
if not required(window SOP_Payment_Entry) then
	call Warning_Dialog, 2351, \"\", \"\", \"\";
	abort script;
end if;


{ Confirm with User }
if ask(getmsg(22019), getmsg(YES), getmsg(CANCEL), \"\") <> ASKBUTTON1 then   {Are you sure you wish to make a PayWay payment?}
	clear 'Card Name';
	run script 'Card Name';
	abort script;
end if;


'(L) PayWay Make Payment' = 1;
"
			UserProperties	""
		}
		EventScript	"SOP_Payment_Entry l_PayWay Payment Summary Code_CHG"		
		{
			Documentation	""
			Script	"if  '(L) PayWay Payment Summary Code' = CH_0 then
	warning getmsg(22016);   {Payment has been successfully made in PayWay.}
	
	'HS SOP Type Credit To Return' of globals = false;
	
	clear table HS_PayWay_Payments_WORK_HIST;
	'HS PayWay Order Number' of table HS_PayWay_Payments_WORK_HIST = 'Document Number';
	'HS PayWay Payment Refund' of table HS_PayWay_Payments_WORK_HIST = HS_PAYWAY_PAYMENT;
	'SOP Type' of table HS_PayWay_Payments_WORK_HIST = SOPTypeToInt('SOP Type');
	'SOP Number' of table HS_PayWay_Payments_WORK_HIST = 'SOP Number';
	if empty('Line Item Sequence') then
		'Sequence Number' of table HS_PayWay_Payments_WORK_HIST = '(L) Next Sequence Number';
	else
		'Sequence Number' of table HS_PayWay_Payments_WORK_HIST = 'Line Item Sequence';
	end if;
	'Customer Number' of table HS_PayWay_Payments_WORK_HIST = 'Customer Number';
	'HS PayWay Customer Reference Number' of table HS_PayWay_Payments_WORK_HIST = 'HS PayWay Customer Reference Number';
	'PayWay Login ID' of table HS_PayWay_Payments_WORK_HIST = 'PayWay Login ID';
	'HS PayWay Date' of table HS_PayWay_Payments_WORK_HIST = sysdate();
	'Amount Paid' of table HS_PayWay_Payments_WORK_HIST = 'Amount Paid';
	clear 'HS PayWay Original Order Number' of table HS_PayWay_Payments_WORK_HIST;
	'HS PayWay Receipt Number' of table HS_PayWay_Payments_WORK_HIST = '(L) PayWay Payment Receipt Number';
	save table HS_PayWay_Payments_WORK_HIST;
	
	'HS SOP Type Credit To Return' of globals = true;
	
	run script 'Insert>> Button I';
	
	abort script;
else
	warning getmsg(22018) + '(L) PayWay Payment Response Text';   {PayWay payment unsuccessful - }
	
	{ Burn the Cash Receipt Number }
	clear '(L) Default Document Number';
	run script '(L) Get Next Receipt Number';
	
	{ Clear the Card Name }
	clear 'Card Name';
	
	abort script;
end if;
"
			UserProperties	""
		}
		EventScript	"SOP_Payment_Entry l_PayWay PrePaid CB_CHG"		
		{
			Documentation	""
			Script	"if '(L) PayWay PrePaid CB' then
	show '(L) PayWay PrePaid Order Number';
	hide '(L) PayWay Payment Button';
	hide '(L) PayWay Refund Button';
	
	'Dummy Record' of table HS_PayWay_SETP = false;
	get table HS_PayWay_SETP;
	if err() <> OKAY then
		clear table HS_PayWay_SETP;
	end if;
	
	'Card Name' = 'Card Name' of table HS_PayWay_SETP;
	run script 'Card Name';
else
	hide '(L) PayWay PrePaid Order Number';
