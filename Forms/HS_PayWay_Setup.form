		EventScript	"PayWay_Setup Card Name_CHG"		
		{
			Documentation	""
			Script	"if empty('Card Name') then
	abort script;
end if;


'Card Name' of table SY_Credit_Card_MSTR = 'Card Name';
get table SY_Credit_Card_MSTR;
if err() <> OKAY then
	restart field;
	if ask(getmsg(2278), getmsg(ADD), getmsg(CANCEL), \"\") = ASKBUTTON1 then
		run script 'Zoom Buttons'[1];
	end if;
	abort script;
else
	if not 'CB Receivable' of table SY_Credit_Card_MSTR then
		call Warning_Dialog, 2573, \"\", \"\", \"\";
		restart field;	
		abort script;
	end if;
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup Lookup Button 1_CHG"		
		{
			Documentation	""
			Script	"local		string		sPath;


pragma (disable warning LiteralStringUsed);

getfile(getmsg(22012), 1, sPath, \"Credentials File (*.q0)|*.q0|\");   {Select a credentials file.}
'PayWay Credentials File' = Path_MakeNative(sPath);

pragma (enable warning LiteralStringUsed);
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup Lookup Button 2_CHG"		
		{
			Documentation	""
			Script	"local	string		sPath;


if Path_SelectPathname(sPath) then
	'PayWay Log Directory' = Path_MakeNative(sPath);
	run script 'PayWay Log Directory';
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup Lookup Button 3_CHG"		
		{
			Documentation	""
			Script	"open form Credit_Card_Lookup return to 'Card Name';
if isopen(form Credit_Card_Lookup) then
	if not empty('Card Name') then
		'Card Name' of window Credit_Card_Lookup of form Credit_Card_Lookup = 'Card Name';
		run script 'Card Name' of window Credit_Card_Lookup of form Credit_Card_Lookup;
	end if;
	run script 'Redisplay Button' of window Credit_Card_Lookup of form Credit_Card_Lookup;
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup OK Button_CHG"		
		{
			Documentation	""
			Script	"if not required(window PayWay_Setup) then
	call Warning_Dialog,2351,\"\",\"\",\"\";
	abort script;
end if;


'Dummy Record' of table HS_PayWay_SETP = false;
change table HS_PayWay_SETP;
copy from window PayWay_Setup to table HS_PayWay_SETP;
save table HS_PayWay_SETP;

clear changes window PayWay_Setup;
close form HS_PayWay_Setup;
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup PayWay Credentials File_CHG"		
		{
			Documentation	""
			Script	"if empty('PayWay Credentials File') then
	abort script;
end if;


if not File_Probe(Path_MakeGeneric('PayWay Credentials File')) then
	warning getmsg(22010);   {Please enter a valid file.}
	restart field;
	abort script;
end if;
"
			UserProperties	""
		}
		EventScript	"PayWay_Setup PayWay Log Directory_CHG"		
		{
			Documentation	""
			Script	"if empty('PayWay Log Directory') then
	abort script;
end if;


if substring('PayWay Log Directory', length('PayWay Log Directory'), 1) = CH_BACKSLASH then
	'PayWay Log Directory' = substring('PayWay Log Directory', 1, length('PayWay Log Directory') - 1);
end if;


if not Path_DoesExist('PayWay Log Directory') then
	warning getmsg(22011);   {Please enter a valid path.}
	restart field;
	abort script;
end if;
"
			UserProperties	""
		}
